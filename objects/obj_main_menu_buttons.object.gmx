if (room_get_name(room)="Creation"){
    var yy,i;yy=y;i=1;
    if (glowing[i]=1) and (hover[i]=1){glow[i]+=1;if (glow[i]&gt;=40) then glowing[i]=-1;}
    if (glowing[i]=-1) and (hover[i]=1){glow[i]-=1;if (glow[i]&lt;=0) then glowing[i]=1;}
