            var nope;nope=0;if (master_volume=0) or (music_volume=0) then nope=1;
            if (nope!=1){audio_sound_gain(snd_royal,0.25*master_volume*music_volume,0);}
            if (nope=1){audio_sound_gain(snd_royal,0,0);}
